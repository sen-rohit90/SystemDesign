        <mxCell id="AqhTQObQEV2LdB_O8yc8-23" value="Step 6" style="rounded=0;whiteSpace=wrap;html=1;fillColor=#f8cecc;strokeColor=#b85450;rotation=-5;" vertex="1" parent="1">
